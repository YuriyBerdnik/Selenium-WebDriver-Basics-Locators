{
  "env": {
    "browser": true,
    "es6": true,
    "node": true,
    "mocha": true,
    "jasmine": true
  },
  "extends": ["airbnb/base",
    "eslint:recommended",
    "plugin:protractor/recommended",
    "plugin:mocha/recommended",
    "plugin:jasmine/recommended"
  ],
  "plugins": ["protractor",
    "mocha",
    "jasmine"
  ],
  "rules": {
    "no-await-in-loop": "off",
    "class-methods-use-this": "off",
    "no-plusplus": "off",
    "eol-last": "off",
    "max-len": [
      "error",
      150,
      {
        "ignoreUrls": true
      }
    ],
    "no-tabs": "off",
    "no-alert": "off",
    "prefer-destructuring": "off",
    "indent": [2, "tab"],
    "no-param-reassign": "off",
    "linebreak-style": 0,
    "new-cap": "off",
    "consistent-return": "off",
    "prefer-arrow-callback": "off",
    "mocha/no-mocha-arrows": "off"
  }
}